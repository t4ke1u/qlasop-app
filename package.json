{
  "name": "optimetable-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:aspida": "aspida --watch",
    "dev:path": "pathpida --ignorepath .gitignore --output ./src/generated",
    "build": "run-s build:*",
    "build:aspida": "aspida",
    "build:path": "pathpida --ignorepath .gitignore --output ./src/generated",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'"
  },
  "dependencies": {
    "@aspida/axios": "^1.12.0",
    "@chakra-ui/next-js": "^2.1.4",
    "@chakra-ui/react": "^2.7.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.1.0",
    "@supabase/supabase-js": "^2.26.0",
    "@t3-oss/env-nextjs": "^0.6.0",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@types/uuid": "^9.0.2",
    "axios": "^1.4.0",
    "encoding": "^0.1.13",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "framer-motion": "^10.12.17",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-icons": "^4.10.1",
    "swr": "^2.2.0",
    "typescript": "5.0.4",
    "uuid": "^9.0.0",
    "zod": "^3.21.4",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.8.0",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.20.1",
    "prettier": "^2.8.8"
  }
}
